{
  "name": "@meteo-parapente-new/meteo-parapente-new-server",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/meteo-parapente-new-server/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "defaultConfiguration": "production",
      "options": {
        "cwd": "apps/meteo-parapente-new-server",
        "parallel": false,
        "commands": [
          { "command": "tsc -p tsconfig.app.json" },
          { "command": "tsc-alias -p tsconfig.app.json" },
          { "command": "node ../../tools/fix-esm-extensions.mjs dist" },
          { "command": "node ../../tools/move-dist-src-up.mjs dist" }
        ]
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "fix-imports": {
      "executor": "@nx/workspace:run-commands",
      "options": {
        "commands": [
          { "command": "tsc-alias -p apps/meteo-parapente-new-server/tsconfig.app.json" },
          { "command": "tsc-esm-fix apps/meteo-parapente-new-server/dist" }
        ]
      }
    },
    "serve": {
      "continuous": true,
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "commands": [
          { "command": "node apps/meteo-parapente-new-server/dist/main.js" }
        ]
      }
    }
  }
}
